<?xml version="1.0" encoding="UTF-8"?>
<feature
      id="de.innot.avreclipse"
      label="AVR Eclipse Plugin"
      version="2.5.0.qualifier"
      provider-name="AVR Eclipse Plugin"
      plugin="de.innot.avreclipse.core">

   <description>
      The AVR Eclipse Plugin is an extension to the C/C++ Development Toolkit (CDT) to develop applications for the ATMEL AVR series of embedded processors.
   </description>

   <license url="http://www.eclipse.org/legal/epl-v10.html">
      Eclipse Public License - v 1.0
   </license>

   <url>
      <update label="AVR Eclipse Plugin Update Site" url="http://avr-eclipse.sourceforge.net/updatesite/"/>
      <discovery label="%discoverySiteName" url="http://avr-eclipse.sourceforge.net/updatesite/"/>
   </url>

   <requires>
      <import plugin="org.eclipse.cdt.core" version="4.0.0" match="greaterOrEqual"/>
      <import plugin="org.eclipse.cdt.managedbuilder.core"/>
      <import plugin="org.eclipse.core.runtime"/>
      <import plugin="org.eclipse.core.resources"/>
      <import plugin="org.eclipse.core.filesystem"/>
      <import plugin="org.eclipse.ui.ide"/>
      <import plugin="org.eclipse.ui"/>
      <import plugin="org.eclipse.cdt.managedbuilder.ui"/>
      <import plugin="org.eclipse.cdt.ui"/>
      <import plugin="org.eclipse.jface"/>
      <import plugin="org.eclipse.ui.console"/>
      <import plugin="org.eclipse.core.expressions"/>
      <import plugin="org.eclipse.ui.workbench"/>
      <import plugin="org.eclipse.ui.forms"/>
      <import plugin="org.eclipse.ui.editors"/>
      <import plugin="org.eclipse.jface.text"/>
      <import plugin="org.eclipse.cdt.core" version="4.0.1" match="greaterOrEqual"/>
      <import plugin="org.eclipse.cdt.make.ui"/>
      <import plugin="org.eclipse.cdt.debug.mi.core"/>
      <import plugin="org.eclipse.cdt.debug.core"/>
      <import plugin="org.eclipse.debug.core"/>
      <import plugin="org.eclipse.cdt.launch"/>
      <import plugin="org.eclipse.cdt.dsf.gdb"/>
      <import plugin="org.eclipse.cdt.dsf"/>
      <import plugin="org.eclipse.cdt.debug.gdbjtag.core"/>
      <import plugin="org.eclipse.core.variables"/>
      <import plugin="org.eclipse.debug.ui"/>
   </requires>

   <plugin
         id="de.innot.avreclipse.root"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

   <plugin
         id="de.innot.avreclipse.core"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

   <plugin
         id="de.innot.avreclipse.core.managedbuildsystem"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

   <plugin
         id="de.innot.avreclipse.ui"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

   <plugin
         id="de.innot.avreclipse.doc"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

   <plugin
         id="de.innot.avreclipse.debug.core"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

   <plugin
         id="de.innot.avreclipse.debug.gdbservers"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

   <plugin
         id="de.innot.avreclipse.debug.ui"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

